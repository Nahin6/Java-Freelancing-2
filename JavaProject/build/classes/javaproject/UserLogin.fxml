<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="744.0" prefWidth="883.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaproject.UserLoginController">
    <children>
        <Label fx:id="label" layoutX="311.0" layoutY="62.0" minHeight="16" minWidth="69" prefHeight="67.0" prefWidth="295.0" text="Login your Account" textFill="#032c0e">
         <font>
            <Font name="System Bold" size="30.0" />
         </font></Label>
      <TextField fx:id="userName" layoutX="342.0" layoutY="223.0" promptText="Username" />
      <Label layoutX="57.0" layoutY="222.0" prefHeight="31.0" prefWidth="261.0" text="Enter your User Name :">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="76.0" layoutY="265.0" prefHeight="34.0" prefWidth="306.0" text="Enter your Password :">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="userPassword" layoutX="342.0" layoutY="266.0" promptText="Password" />
     
      <Button layoutX="382.0" layoutY="372.0" mnemonicParsing="false" onAction="#clickLogInButtonOnAction" prefHeight="42.0" prefWidth="80.0" text="Login">
         <font>
            <Font name="System Bold" size="19.0" />
         </font></Button>
      <Button layoutX="382.0" layoutY="467.0" mnemonicParsing="false" onAction="#clickSignUpButtonOnAction" text="Sign up">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
      <Label layoutX="339.0" layoutY="427.0" text="Dont have account?" textFill="#900f0f">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Label fx:id="loginLabel" layoutX="167.0" layoutY="145.0" prefHeight="50.0" prefWidth="484.0" />
      <ComboBox fx:id="allUserInComboBox" layoutX="341.0" layoutY="313.0" prefHeight="31.0" prefWidth="187.0" />
    </children>
</AnchorPane>
